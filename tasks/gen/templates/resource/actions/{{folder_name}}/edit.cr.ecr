class <%= pluralized_name %>::Edit < BrowserAction
  <%= route("Edit") %> do
    <%= underscored_resource %> = <%= query_class %>.find(<%= resource_id_method_name %>)
    html EditPage,
      operation: Save<%= resource %>.new(<%= underscored_resource %>),
      <%= underscored_resource %>: <%= underscored_resource %>
  end
end
